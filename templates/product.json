            "liquid": "{% assign notes = product.metafields.custom.notes.value %}\n\n{% if notes != blank %}\n  <div class=\"notes-block\">\n    <div class=\"notes-title\">NOTES</div>\n\n    <div class=\"notes-slider-wrap\">\n      <button class=\"notes-arrow notes-prev\" type=\"button\" aria-label=\"Previous notes\">‹</button>\n\n      <div class=\"notes-slider\" data-notes-slider>\n        {% for file in notes %}\n          {% assign img = file | image_url: width: 240 %}\n          <div class=\"notes-item\">\n            <div class=\"notes-img\">\n              <img\n                src=\"{{ img }}\"\n                alt=\"{{ file.alt | default: 'Note' }}\"\n                loading=\"lazy\"\n                width=\"120\"\n                height=\"120\"\n              >\n            </div>\n\n            {%- comment -%}\n              If you don't have labels, we can try using alt text.\n              Add alt text to the file in Shopify admin for best results.\n            {%- endcomment -%}\n            <div class=\"notes-label\">{{ file.alt | default: '' }}</div>\n          </div>\n        {% endfor %}\n      </div>\n\n      <button class=\"notes-arrow notes-next\" type=\"button\" aria-label=\"Next notes\">›</button>\n    </div>\n  </div>\n\n  <style>\n    .notes-block { margin: 16px 0; }\n    .notes-title { font-size: 12px; letter-spacing: .12em; text-transform: uppercase; margin-bottom: 10px; }\n\n    .notes-slider-wrap { position: relative; display: flex; align-items: center; gap: 8px; }\n\n    .notes-arrow{\n      border: 0; background: transparent; cursor: pointer;\n      font-size: 22px; line-height: 1; padding: 6px 10px;\n    }\n\n    .notes-slider{\n      display: flex;\n      gap: 18px;\n      overflow-x: auto;\n      scroll-snap-type: x mandatory;\n      -webkit-overflow-scrolling: touch;\n      scrollbar-width: none;\n      padding: 2px 2px 8px;\n      width: 100%;\n    }\n    .notes-slider::-webkit-scrollbar{ display:none; }\n\n    .notes-item{ flex: 0 0 auto; scroll-snap-align: start; text-align: center; }\n    .notes-img{\n      width: 74px; height: 74px; border-radius: 999px; overflow: hidden;\n      background: #f3f3f3;\n      display: grid; place-items: center;\n    }\n    .notes-img img{ width: 100%; height: 100%; object-fit: cover; display:block; }\n\n    .notes-label{\n      margin-top: 8px;\n      font-size: 12px;\n      line-height: 1.2;\n      white-space: nowrap;\n    }\n\n    /* Mobile tweaks */\n    @media (max-width: 480px){\n      .notes-img{ width: 62px; height: 62px; }\n      .notes-slider{ gap: 14px; }\n      .notes-arrow{ display: none; } /* swipe only */\n    }\n  </style>\n\n  <script>\n    (function(){\n      const slider = document.querySelector('[data-notes-slider]');\n      if(!slider) return;\n\n      const prev = slider.parentElement.querySelector('.notes-prev');\n      const next = slider.parentElement.querySelector('.notes-next');\n\n      function scrollByCard(dir){\n        const firstItem = slider.querySelector('.notes-item');\n        if(!firstItem) return;\n        const gap = parseFloat(getComputedStyle(slider).gap || 0);\n        const step = firstItem.getBoundingClientRect().width + gap;\n        slider.scrollBy({ left: dir * step, behavior: 'smooth' });\n      }\n\n      prev && prev.addEventListener('click', () => scrollByCard(-1));\n      next && next.addEventListener('click', () => scrollByCard(1));\n    })();\n  </script>\n{% endif %}"
