        "liquid": "{% assign sections = product.metafields.custom.image_and_features.value %}\n\n{% if sections != blank %}\n  <section class=\"pf-premium\">\n    <div class=\"pf-premium__inner\">\n\n      <!-- DESKTOP -->\n      <div class=\"pf-premium__desktop\">\n        {% for item in sections %}\n          {% assign is_even = forloop.index | modulo: 2 %}\n\n          <article class=\"pf-block {% if is_even == 0 %}pf-block--reverse{% endif %}\">\n            <div class=\"pf-copy\">\n              {% if item.title != blank %}\n                <h3 class=\"pf-eyebrow\">{{ item.title }}</h3>\n              {% endif %}\n              <div class=\"pf-rich\">\n                {{ item.text | metafield_tag }}\n              </div>\n            </div>\n\n            <div class=\"pf-media\">\n              {% if item.image != blank %}\n                <img\n                  src=\"{{ item.image | image_url: width: 2000 }}\"\n                  alt=\"{{ item.title | escape }}\"\n                  loading=\"lazy\"\n                  width=\"2000\"\n                  height=\"2000\"\n                >\n              {% endif %}\n            </div>\n          </article>\n        {% endfor %}\n      </div>\n\n      <!-- MOBILE (Accordion) -->\n      <div class=\"pf-premium__mobile\">\n        {% for item in sections %}\n\n          {% capture accordion_content %}\n            <div class=\"pf-mob-content\">\n              {% if item.image != blank %}\n                <div class=\"pf-mob-media\">\n                  <img\n                    src=\"{{ item.image | image_url: width: 1400 }}\"\n                    alt=\"{{ item.title | escape }}\"\n                    loading=\"lazy\"\n                    width=\"1400\"\n                    height=\"1400\"\n                  >\n                </div>\n              {% endif %}\n\n              <div class=\"pf-mob-rich pf-truncate\" data-truncate=\"true\">\n                {{ item.text | metafield_tag }}\n              </div>\n\n              <button type=\"button\" class=\"pf-more\" aria-expanded=\"false\">\n                Read more\n              </button>\n            </div>\n          {% endcapture %}\n\n          {%- render 'accordion',\n            title: item.title,\n            content: accordion_content,\n            prose_content: false,\n            block: block\n          -%}\n\n        {% endfor %}\n      </div>\n\n    </div>\n  </section>\n{% endif %}\n\n\n<style>\n/* =========================\n   PREMIUM WRAPPER\n========================= */\n.pf-premium{\n  padding: 0 !important;\n  margin: 0 !important;\n}\n\n.pf-premium__inner{\n  max-width: 1200px;\n  margin: 0 auto;\n  padding: 0 !important;\n}\n\n/* Hide / show blocks */\n.pf-premium__desktop{ display: none; }\n.pf-premium__mobile{ display: block; }\n\n@media (min-width: 992px){\n  .pf-premium__desktop{ display: block; }\n  .pf-premium__mobile{ display: none; }\n}\n\n/* =========================\n   DESKTOP PREMIUM LAYOUT\n========================= */\n.pf-block{\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  align-items: center;\n  gap: 70px;\n  padding: 0 !important;\n  margin: 0 0 90px 0 !important;\n}\n\n.pf-block:last-child{ margin-bottom: 0 !important; }\n\n.pf-block--reverse .pf-copy{ order: 2; }\n.pf-block--reverse .pf-media{ order: 1; }\n\n/* Typography = premium */\n.pf-eyebrow{\n  font-family: \"Inter\", -apple-system, sans-serif;\n  font-size: 12px;\n  letter-spacing: 2px;\n  text-transform: uppercase;\n  font-weight: 600;\n  margin: 0 0 18px 0 !important;\n  color: rgba(0,0,0,0.65);\n}\n\n.pf-rich{\n  font-family: \"Inter\", -apple-system, sans-serif;\n  font-size: 18px;\n  line-height: 1.95;\n  font-weight: 300;\n  color: rgba(0,0,0,0.82);\n  margin: 0 !important;\n}\n\n/* Remove default rich text margins */\n.pf-rich p,\n.pf-rich h1,\n.pf-rich h2,\n.pf-rich h3,\n.pf-rich h4,\n.pf-rich ul,\n.pf-rich ol{\n  margin: 0 0 14px 0 !important;\n}\n\n.pf-rich p:last-child,\n.pf-rich ul:last-child,\n.pf-rich ol:last-child{ margin-bottom: 0 !important; }\n\n.pf-rich ul{ padding-left: 18px !important; }\n.pf-rich li{ margin: 0 0 8px 0 !important; }\n\n/* Media premium treatment */\n.pf-media{\n  position: relative;\n  border-radius: 14px;\n  overflow: hidden;\n  background: #f6f6f6;\n}\n\n.pf-media img{\n  width: 100%;\n  height: auto;\n  display: block;\n  transform: scale(1.01);\n}\n\n/* subtle “premium” overlay */\n.pf-media::after{\n  content:\"\";\n  position:absolute;\n  inset:0;\n  background: radial-gradient(60% 60% at 50% 35%, rgba(255,255,255,0.18) 0%, rgba(0,0,0,0.10) 65%, rgba(0,0,0,0.18) 100%);\n  pointer-events:none;\n}\n\n/* =========================\n   MOBILE ACCORDION PREMIUM\n========================= */\n.pf-premium__mobile{\n  padding: 0 !important;\n  margin: 0 !important;\n}\n\n/* Remove spacing caused by accordion container (generic overrides) */\n.pf-premium__mobile .accordion,\n.pf-premium__mobile details,\n.pf-premium__mobile summary{\n  margin: 0 !important;\n}\n\n.pf-mob-content{\n  padding: 0 !important;\n  margin: 0 !important;\n}\n\n.pf-mob-media{\n  border-radius: 12px;\n  overflow: hidden;\n  background: #f6f6f6;\n  margin: 12px 0 12px 0 !important;\n}\n\n.pf-mob-media img{\n  width: 100%;\n  height: auto;\n  display: block;\n}\n\n/* Rich text spacing reset */\n.pf-mob-rich,\n.pf-mob-rich *{\n  margin-top: 0 !important;\n}\n\n.pf-mob-rich p,\n.pf-mob-rich ul,\n.pf-mob-rich ol{\n  margin: 0 0 12px 0 !important;\n}\n\n.pf-mob-rich p:last-child{ margin-bottom: 0 !important; }\n\n/* Truncate */\n.pf-truncate[data-truncate=\"true\"]{\n  max-height: 92px;\n  overflow: hidden;\n}\n\n.pf-truncate[data-truncate=\"false\"]{ max-height: none; }\n\n/* Read more button premium */\n.pf-more{\n  margin-top: 10px;\n  padding: 0;\n  background: transparent;\n  border: 0;\n  cursor: pointer;\n  font: inherit;\n  text-decoration: none;\n  color: rgba(0,0,0,0.85);\n  border-bottom: 1px solid rgba(0,0,0,0.35);\n}\n\n/* Optional: remove if you don’t want underline hover */\n.pf-more:hover{ border-bottom-color: rgba(0,0,0,0.75); }\n\n</style>\n\n<script>\ndocument.addEventListener(\"click\", function(e){\n  const btn = e.target.closest(\".pf-more\");\n  if(!btn) return;\n\n  const wrap = btn.closest(\".pf-mob-content\");\n  const box = wrap.querySelector(\".pf-truncate\");\n  const expanded = btn.getAttribute(\"aria-expanded\") === \"true\";\n\n  if(expanded){\n    box.setAttribute(\"data-truncate\",\"true\");\n    btn.setAttribute(\"aria-expanded\",\"false\");\n    btn.textContent = \"Read more\";\n  } else {\n    box.setAttribute(\"data-truncate\",\"false\");\n    btn.setAttribute(\"aria-expanded\",\"true\");\n    btn.textContent = \"Read less\";\n  }\n});\n</script>",
