            "liquid": "{% if product.metafields.custom.features.value %}\n  <style>\n    .pf-feature-slider-wrapper {\n      margin: 10px 0 18px;\n      overflow: hidden;\n      position: relative;\n    }\n    \n    .pf-feature-slider-row {\n      display: flex;\n      gap: 8px;\n      animation: scroll-left 30s linear infinite;\n      width: fit-content;\n    }\n    \n    .pf-feature-slider-row:nth-child(2) {\n      margin-top: 8px;\n      animation: scroll-right 30s linear infinite;\n    }\n    \n    /* Pause animation on hover */\n    .pf-feature-slider-wrapper:hover .pf-feature-slider-row {\n      animation-play-state: paused;\n    }\n    \n    .pf-feature-chip {\n      display: inline-flex;\n      align-items: center;\n      column-gap: 4px;\n      padding: 8px 16px;\n      border-radius: 0px;\n      background: #fff;\n      border: 1px solid #AC1D21;\n      font-size: 12px;\n      line-height: 1.4;\n      color: #1c1c1c;\n      white-space: nowrap;\n      flex-shrink: 0;\n    }\n    \n    .pf-feature-chip-main {\n      font-weight: 600;\n      color: #000;\n    }\n    \n    /* Scroll animations */\n    @keyframes scroll-left {\n      0% {\n        transform: translateX(0);\n      }\n      100% {\n        transform: translateX(-50%);\n      }\n    }\n    \n    @keyframes scroll-right {\n      0% {\n        transform: translateX(-50%);\n      }\n      100% {\n        transform: translateX(0);\n      }\n    }\n    \n    /* Tablet */\n    @media (max-width: 768px) {\n      .pf-feature-chip {\n        padding: 7px 14px;\n        font-size: 11px;\n      }\n      \n      .pf-feature-slider-row {\n        animation-duration: 25s;\n      }\n    }\n    \n    /* Mobile */\n    @media (max-width: 480px) {\n      .pf-feature-chip {\n        padding: 6px 12px;\n        font-size: 10px;\n      }\n      \n      .pf-feature-slider-row {\n        animation-duration: 20s;\n        gap: 6px;\n      }\n      \n      .pf-feature-slider-row:nth-child(2) {\n        margin-top: 6px;\n      }\n    }\n  </style>\n  \n  <div class=\"pf-feature-slider-wrapper\">\n    {% assign features = product.metafields.custom.features.value %}\n    {% assign half = features.size | divided_by: 2.0 | ceil %}\n    \n    <!-- First Row -->\n    <div class=\"pf-feature-slider-row\">\n      {% for feature in features limit: half %}\n        {% assign parts = feature | split: \"|\" %}\n        <div class=\"pf-feature-chip\">\n          <span class=\"pf-feature-chip-main\">{{ parts[0] | strip }}</span>\n        </div>\n      {% endfor %}\n      <!-- Duplicate for seamless loop -->\n      {% for feature in features limit: half %}\n        {% assign parts = feature | split: \"|\" %}\n        <div class=\"pf-feature-chip\">\n          <span class=\"pf-feature-chip-main\">{{ parts[0] | strip }}</span>\n        </div>\n      {% endfor %}\n    </div>\n    \n    <!-- Second Row -->\n    <div class=\"pf-feature-slider-row\">\n      {% for feature in features offset: half %}\n        {% assign parts = feature | split: \"|\" %}\n        <div class=\"pf-feature-chip\">\n          <span class=\"pf-feature-chip-main\">{{ parts[0] | strip }}</span>\n        </div>\n      {% endfor %}\n      <!-- Duplicate for seamless loop -->\n      {% for feature in features offset: half %}\n        {% assign parts = feature | split: \"|\" %}\n        <div class=\"pf-feature-chip\">\n          <span class=\"pf-feature-chip-main\">{{ parts[0] | strip }}</span>\n        </div>\n      {% endfor %}\n    </div>\n  </div>\n{% endif %}"
