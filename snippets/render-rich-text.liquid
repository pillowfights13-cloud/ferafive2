{%- if rich_text_data != blank -%}
  {%- for child in rich_text_data.children -%}
    
    {%- if child.type == 'paragraph' -%}
      <p>
        {%- for text_node in child.children -%}
          {%- if text_node.type == 'text' -%}
            {{ text_node.value }}
          {%- endif -%}
        {%- endfor -%}
      </p>
    {%- endif -%}
    
    {%- if child.type == 'list' -%}
      {%- if child.listType == 'ordered' -%}
        <ol>
      {%- elsif child.listType == 'unordered' -%}
        <ul>
      {%- endif -%}
      
      {%- for item in child.children -%}
        {%- if item.type == 'list-item' -%}
          <li>
            {%- for text_node in item.children -%}
              {%- if text_node.type == 'text' -%}
                {{ text_node.value }}
              {%- endif -%}
            {%- endfor -%}
          </li>
        {%- endif -%}
      {%- endfor -%}
      
      {%- if child.listType == 'ordered' -%}
        </ol>
      {%- elsif child.listType == 'unordered' -%}
        </ul>
      {%- endif -%}
    {%- endif -%}
    
  {%- endfor -%}
{%- endif -%}